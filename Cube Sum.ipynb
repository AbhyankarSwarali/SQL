    "def cube_sum():\n",
    "    num = int(input(\"Enter a number:\"))\n",
    "    x = 1\n",
    "    Sum = 0\n",
    "    while x < num: \n",
    "        cube = x**3\n",
    "        Sum += cube\n",
    "        x += 1\n",
    "    return Sum\n",
    "cube_sum()"
   ]
